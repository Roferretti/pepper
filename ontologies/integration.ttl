@prefix : <http://cltl.nl/leolani/integration/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@base <http://cltl.nl/leolani/integration/> .

<http://cltl.nl/leolani/integration/> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#broadMatch
<http://www.w3.org/2004/02/skos/core#broadMatch> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
<http://www.w3.org/2004/02/skos/core#closeMatch> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrowMatch
<http://www.w3.org/2004/02/skos/core#narrowMatch> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#category
<http://www.w3.org/ns/prov#category> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#component
<http://www.w3.org/ns/prov#component> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#definition
<http://www.w3.org/ns/prov#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#inverse
<http://www.w3.org/ns/prov#inverse> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#qualifiedForm
<http://www.w3.org/ns/prov#qualifiedForm> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

### n2mu:acknowledge
n2mu:acknowledge rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf n2mu:perceive ;
                                          rdfs:domain n2mu:robot;
                                          rdfs:range n2mu:smell,
                                                     n2mu:taste,
                                                     n2mu:touch;
                                          owl:propertyDisjointWith n2mu:experience .


### n2mu:born-in
n2mu:born-in rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf n2mu:be-from ;
                                     rdfs:range n2mu:location;
                                      rdfs:domain n2mu:person.


### n2mu:cook-by
n2mu:cook-by rdf:type owl:ObjectProperty ;
                                       rdfs:domain n2mu:dish,
                                                   n2mu:food;
                                       rdfs:range n2mu:agent.


### n2mu:dislike
n2mu:dislike rdf:type owl:ObjectProperty ;
                                       rdfs:domain n2mu:agent;
                                       rdfs:range n2mu:agent,
                                                  n2mu:interest.


### n2mu:experience
n2mu:experience rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf n2mu:perceive ;
                                         rdfs:domain n2mu:person;
                                         rdfs:range n2mu:smell,
                                                    n2mu:taste,
                                                    n2mu:touch.


### n2mu:favorite
n2mu:favorite rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf n2mu:like ;
                                       owl:inverseOf n2mu:favorite-of ;
                                       rdfs:domain n2mu:agent;
                                       rdfs:range n2mu:interest.


### n2mu:favorite-of
n2mu:favorite-of rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf n2mu:like-by ;
                                       rdfs:range n2mu:agent;
                                       rdfs:domain n2mu:interest.


### n2mu:in
n2mu:in rdf:type owl:ObjectProperty ,
                                          owl:TransitiveProperty ;
                                 rdfs:domain n2mu:city ;
                                 rdfs:range n2mu:country .


### n2mu:be-child-of
n2mu:be-child-of rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf n2mu:be-family-of ;
                                        rdfs:domain n2mu:agent;
                                        rdfs:range n2mu:agent.


### n2mu:be-colleague-of
n2mu:be-colleague-of rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf n2mu:know ;
                                            rdfs:domain [ owl:intersectionOf ( n2mu:agent
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty n2mu:be-member-of ;
                                                                                 owl:someValuesFrom n2mu:institution
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                            rdfs:range [ owl:intersectionOf ( n2mu:agent
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty n2mu:be-member-of ;
                                                                                owl:someValuesFrom n2mu:institution
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ] .


### n2mu:be-family-of
n2mu:be-family-of rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf n2mu:know ;
                                         rdfs:domain n2mu:person;
                                         rdfs:range n2mu:person.


### n2mu:be-friends-with
n2mu:be-friends-with rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf n2mu:know ;
                                         rdfs:domain n2mu:person;
                                         rdfs:range n2mu:person.


### n2mu:be-from
n2mu:be-from rdf:type owl:ObjectProperty ;
                                     rdfs:domain n2mu:agent,
                                                 n2mu:cuisine;
                                     rdfs:range n2mu:location.


### n2mu:be-member-of
n2mu:be-member-of rdf:type owl:ObjectProperty ;
                                         rdfs:domain n2mu:agent;
                                         rdfs:range n2mu:institution.


### n2mu:be-parent-of
n2mu:be-parent-of rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf n2mu:be-family-of ;
                                         rdfs:domain n2mu:person;
                                         rdfs:range n2mu:person.


### n2mu:know
n2mu:know rdf:type owl:ObjectProperty ;
                                    rdfs:domain n2mu:agent;
                                    rdfs:range n2mu:agent.


### n2mu:like-by
n2mu:like-by rdf:type owl:ObjectProperty ;
                                    rdfs:range n2mu:agent;
                                    rdfs:domain n2mu:agent,
                                               n2mu:interest.


### n2mu:like
n2mu:like rdf:type owl:ObjectProperty ;
                                    rdfs:domain n2mu:agent;
                                    rdfs:range n2mu:agent,
                                               n2mu:interest.


### n2mu:live-in
n2mu:live-in rdf:type owl:ObjectProperty ;
                                      rdfs:domain n2mu:agent;
                                      rdfs:range n2mu:location.


### n2mu:locate-in
n2mu:locate-in rdf:type owl:ObjectProperty ;
                                      rdfs:domain n2mu:institution;
                                      rdfs:range n2mu:location.


### n2mu:manufacture-in
n2mu:manufacture-in rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf n2mu:be-from ;
                                             rdfs:domain n2mu:robot ;
                                             rdfs:range n2mu:location.


### n2mu:own
n2mu:own rdf:type owl:ObjectProperty ;
                                   rdfs:domain n2mu:agent,
                                               n2mu:person;
                                   rdfs:range n2mu:object.


### n2mu:perceive
n2mu:perceive rdf:type owl:ObjectProperty ;
                                       rdfs:domain n2mu:agent;
                                       rdfs:range n2mu:sensor.


### n2mu:play
n2mu:play rdf:type owl:ObjectProperty ;
                                    rdfs:domain n2mu:agent;
                                    rdfs:range n2mu:song,
                                               n2mu:sport.


### n2mu:read-by
n2mu:read-by rdf:type owl:ObjectProperty ;
                                     rdfs:domain n2mu:book;
                                     rdfs:range n2mu:agent.


### n2mu:sense
n2mu:sense rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf n2mu:perceive ;
                                    rdfs:domain n2mu:agent.


### n2mu:share-interest
n2mu:share-interest rdf:type owl:ObjectProperty ;
                                            rdfs:domain [ owl:intersectionOf ( n2mu:agent
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty n2mu:like ;
                                                                                 owl:someValuesFrom n2mu:interest
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                            rdfs:range [ owl:intersectionOf ( n2mu:agent
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty n2mu:like ;
                                                                                owl:someValuesFrom n2mu:interest
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ] .


### n2mu:study-at
n2mu:study-at rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf n2mu:be-member-of ;
                                        rdfs:domain n2mu:agent;
                                          rdfs:range n2mu:institution.


### n2mu:travel-to
n2mu:travel-to rdf:type owl:ObjectProperty ;
                                          rdfs:domain n2mu:agent;
                                          rdfs:range n2mu:location.


### n2mu:work-at
n2mu:work-at rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf n2mu:be-member-of ;
                                      rdfs:domain n2mu:agent;
                                      rdfs:range n2mu:institution.


### n2mu:write-by
n2mu:write-by rdf:type owl:ObjectProperty ;
                                        rdfs:domain n2mu:book;
                                      rdfs:range n2mu:person.


###  http://groundedannotationframework.org/gaf#containsDenotation
<http://groundedannotationframework.org/gaf#containsDenotation> rdf:type owl:ObjectProperty ;
                                                                rdfs:domain <http://groundedannotationframework.org/gaf#Mention> ;
                                                                rdfs:range <http://groundedannotationframework.org/gaf#Instance> .


###  http://groundedannotationframework.org/gaf#denotedBy
<http://groundedannotationframework.org/gaf#denotedBy> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://groundedannotationframework.org/gaf#denotedIn> ;
                                                       rdfs:domain <http://groundedannotationframework.org/gaf#Instance> ;
                                                       rdfs:range <http://groundedannotationframework.org/gaf#Mention> .


###  http://groundedannotationframework.org/gaf#denotedIn
<http://groundedannotationframework.org/gaf#denotedIn> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <http://groundedannotationframework.org/gaf#Instance> ;
                                                       rdfs:range <http://groundedannotationframework.org/gaf#Mention> .


###  http://groundedannotationframework.org/gaf#denotes
<http://groundedannotationframework.org/gaf#denotes> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://groundedannotationframework.org/gaf#containsDenotation> ;
                                                     rdfs:domain <http://groundedannotationframework.org/gaf#Mention> ;
                                                     rdfs:range <http://groundedannotationframework.org/gaf#Instance> .


###  http://groundedannotationframework.org/grasp#contextProperty
<http://groundedannotationframework.org/grasp#contextProperty> rdf:type owl:ObjectProperty ;
                                                               rdfs:comment "Context properties connect Context to other SEM Core entities." ;
                                                               rdfs:label "context property" .


###  http://groundedannotationframework.org/grasp#hasAttribution
<http://groundedannotationframework.org/grasp#hasAttribution> rdf:type owl:ObjectProperty ;
                                                              owl:inverseOf <http://groundedannotationframework.org/grasp#isAttributionFor> ;
                                                              rdfs:domain <http://groundedannotationframework.org/gaf#Mention> ;
                                                              rdfs:range <http://groundedannotationframework.org/grasp#Attribution> .


###  http://groundedannotationframework.org/grasp#hasContext
<http://groundedannotationframework.org/grasp#hasContext> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://semanticweb.cs.vu.nl/2009/11/sem/eventProperty> ;
                                                          rdfs:domain <http://groundedannotationframework.org/gaf#Instance> ,
                                                                      <http://groundedannotationframework.org/grasp#Chat> ,
                                                                      <http://groundedannotationframework.org/grasp#Visual> ,
                                                                      <http://groundedannotationframework.org/grasp#Utterance> ,
                                                                      <http://groundedannotationframework.org/grasp#Detection> ,
                                                                      <http://groundedannotationframework.org/grasp#Statement> ,
                                                                      <http://groundedannotationframework.org/grasp#Experience> ,
                                                                      <http://semanticweb.cs.vu.nl/2009/11/sem/Event> ;
                                                          rdfs:range <http://groundedannotationframework.org/grasp#Context> .


###  http://groundedannotationframework.org/grasp#hasDetection
<http://groundedannotationframework.org/grasp#hasDetection> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://groundedannotationframework.org/grasp#contextProperty> ;
                                                            rdfs:domain <http://groundedannotationframework.org/grasp#Context> ;
                                                            rdfs:range n2mu:object,
                                                                       n2mu:person.


###  http://groundedannotationframework.org/grasp#isAttributionFor
<http://groundedannotationframework.org/grasp#isAttributionFor> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                                rdfs:domain <http://groundedannotationframework.org/grasp#Attribution> ;
                                                                rdfs:range <http://groundedannotationframework.org/gaf#Mention> .


###  http://groundedannotationframework.org/grasp#wasAttributedTo
<http://groundedannotationframework.org/grasp#wasAttributedTo> rdf:type owl:ObjectProperty ;
                                                               rdfs:domain <http://groundedannotationframework.org/gaf#Mention> ;
                                                               rdfs:range n2mu:sensor,
                                                                          <http://groundedannotationframework.org/grasp#Source> ,
                                                                          <http://semanticweb.cs.vu.nl/2009/11/sem/Actor> ,
                                                                          [ owl:intersectionOf ( n2mu:agent
                                                                                                 <http://groundedannotationframework.org/gaf#Instance>
                                                                                                 <http://groundedannotationframework.org/grasp#Source>
                                                                                                 <http://semanticweb.cs.vu.nl/2009/11/sem/Actor>
                                                                                               ) ;
                                                                            rdf:type owl:Class
                                                                          ] ,
                                                                          [ owl:intersectionOf ( n2mu:sensor
                                                                                                 <http://groundedannotationframework.org/gaf#Instance>
                                                                                                 <http://groundedannotationframework.org/grasp#Source>
                                                                                                 <http://semanticweb.cs.vu.nl/2009/11/sem/Actor>
                                                                                               ) ;
                                                                            rdf:type owl:Class
                                                                          ] .


###  http://semanticweb.cs.vu.nl/2009/11/sem/eventProperty
<http://semanticweb.cs.vu.nl/2009/11/sem/eventProperty> rdf:type owl:ObjectProperty ;
                                                        rdfs:comment "Event properties connect Events to other SEM Core entities." ;
                                                        rdfs:label "event property" .


###  http://semanticweb.cs.vu.nl/2009/11/sem/hasActor
<http://semanticweb.cs.vu.nl/2009/11/sem/hasActor> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://semanticweb.cs.vu.nl/2009/11/sem/eventProperty> ;
                                                   rdfs:domain <http://groundedannotationframework.org/grasp#Chat> ,
                                                               <http://groundedannotationframework.org/grasp#Utterance> ,
                                                               <http://semanticweb.cs.vu.nl/2009/11/sem/Event> ;
                                                   rdfs:range <http://semanticweb.cs.vu.nl/2009/11/sem/Actor> .


###  http://semanticweb.cs.vu.nl/2009/11/sem/hasPlace
<http://semanticweb.cs.vu.nl/2009/11/sem/hasPlace> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://groundedannotationframework.org/grasp#contextProperty> ;
                                                   rdfs:domain <http://groundedannotationframework.org/grasp#Context> ;
                                                   rdfs:range n2mu:location,
                                                              [ owl:intersectionOf ( <http://groundedannotationframework.org/gaf#Instance>
                                                                                     <http://semanticweb.cs.vu.nl/2009/11/sem/Place>
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ] .


###  http://semanticweb.cs.vu.nl/2009/11/sem/hasSubEvent
<http://semanticweb.cs.vu.nl/2009/11/sem/hasSubEvent> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://semanticweb.cs.vu.nl/2009/11/sem/eventProperty> ;
                                                      rdfs:domain <http://groundedannotationframework.org/grasp#Chat> ,
                                                                  <http://groundedannotationframework.org/grasp#Visual> ,
                                                                  <http://semanticweb.cs.vu.nl/2009/11/sem/Event> ;
                                                      rdfs:range <http://groundedannotationframework.org/grasp#Detection> ,
                                                                 <http://groundedannotationframework.org/grasp#Utterance> ,
                                                                 <http://semanticweb.cs.vu.nl/2009/11/sem/Event> .


###  http://semanticweb.cs.vu.nl/2009/11/sem/hasTime
<http://semanticweb.cs.vu.nl/2009/11/sem/hasTime> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://groundedannotationframework.org/grasp#contextProperty> ;
                                                  rdfs:domain <http://groundedannotationframework.org/grasp#Context> ;
                                                  rdfs:range <http://semanticweb.cs.vu.nl/2009/11/sem/Time> .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#hadDerivation
<http://www.w3.org/ns/prov#hadDerivation> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://www.w3.org/ns/prov#wasDerivedFrom> .


###  http://www.w3.org/ns/prov#wasDerivedFrom
<http://www.w3.org/ns/prov#wasDerivedFrom> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain <http://groundedannotationframework.org/gaf#Mention> ;
                                           rdfs:range <http://groundedannotationframework.org/grasp#Detection> ,
                                                      <http://groundedannotationframework.org/grasp#Utterance> ;
                                           rdfs:comment "The more specific subproperties of prov:wasDerivedFrom (i.e., prov:wasQuotedFrom, prov:wasRevisionOf, prov:hadPrimarySource) should be used when applicable."@en ;
                                           rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                                           rdfs:label "wasDerivedFrom" ;
                                           <http://www.w3.org/ns/prov#category> "starting-point" ;
                                           <http://www.w3.org/ns/prov#component> "derivations" ;
                                           <http://www.w3.org/ns/prov#definition> "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity."@en ;
                                           <http://www.w3.org/ns/prov#inverse> "hadDerivation" ;
                                           <http://www.w3.org/ns/prov#qualifiedForm> <http://www.w3.org/ns/prov#Derivation> .


#################################################################
#    Data properties
#################################################################

### n2mu:birthday
n2mu:birthday rdf:type owl:DatatypeProperty ;
                                       rdfs:domain n2mu:agent.


### n2mu:name
n2mu:name rdf:type owl:DatatypeProperty ;
                                   rdfs:domain n2mu:agent.


#################################################################
#    Classes
#################################################################

###  http://cltl.nl/leolani/integration/chair
:chair rdf:type owl:Class ;
       rdfs:subClassOf n2mu:object.


### n2mu:Agent
n2mu:agent rdf:type owl:Class;
                                    owl:equivalentClass <http://xmlns.com/foaf/0.1/Agent> .


### n2mu:Book
n2mu:book rdf:type owl:Class;
                                   rdfs:subClassOf n2mu:interest-occurrence.


### n2mu:Company
n2mu:company rdf:type owl:Class;
                                      rdfs:subClassOf n2mu:institution.


### n2mu:Cuisine
n2mu:cuisine rdf:type owl:Class;
                                      rdfs:subClassOf n2mu:interest-field.


### n2mu:Dish
n2mu:dish rdf:type owl:Class;
                                   rdfs:subClassOf n2mu:interest-occurrence.

### n2mu:Food
n2mu:food rdf:type owl:Class;
                                   rdfs:subClassOf n2mu:interest-occurrence.


### n2mu:Electricity
n2mu:electricity rdf:type owl:Class;
                                          rdfs:subClassOf n2mu:sensor.


### n2mu:Equilibrium
n2mu:equilibrium rdf:type owl:Class;
                                          rdfs:subClassOf n2mu:sensor.


### n2mu:film-genre
n2mu:film-genre rdf:type owl:Class;
                                        rdfs:subClassOf n2mu:interest-field.


### n2mu:Gravity
n2mu:gravity rdf:type owl:Class;
                                      rdfs:subClassOf n2mu:sensor.


### n2mu:Institution
n2mu:institution rdf:type owl:Class.


### n2mu:Interest
n2mu:interest rdf:type owl:Class;
                                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                                             owl:onProperty n2mu:like-by ;
                                                             owl:someValuesFrom <http://xmlns.com/foaf/0.1/Agent>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty [ owl:inverseOf n2mu:like
                                                                            ] ;
                                                             owl:someValuesFrom <http://xmlns.com/foaf/0.1/Agent>
                                                           ] .


### n2mu:interest-field
n2mu:interest-field rdf:type owl:Class;
                                            rdfs:subClassOf n2mu:interest.


### n2mu:interest-occurrence
n2mu:interest-occurrence rdf:type owl:Class;
                                                 rdfs:subClassOf n2mu:interest.


### n2mu:Light
n2mu:light rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:sensor.


### n2mu:literature-genre
n2mu:literature-genre rdf:type owl:Class;
                                              rdfs:subClassOf n2mu:interest-field.


### n2mu:Location
n2mu:location rdf:type owl:Class;
                                       owl:equivalentClass <http://schema.org/Place> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty [ owl:inverseOf n2mu:be-from
                                                                            ] ;
                                                             owl:someValuesFrom <http://xmlns.com/foaf/0.1/Agent>
                                                           ] .


### n2mu:Movie
n2mu:movie rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:interest-occurrence.


### n2mu:music-genre
n2mu:music-genre rdf:type owl:Class;
                                         rdfs:subClassOf n2mu:interest-field.


### n2mu:Object
n2mu:object rdf:type owl:Class.


### n2mu:Pain
n2mu:pain rdf:type owl:Class;
                                   rdfs:subClassOf n2mu:sensor.


### n2mu:Person
n2mu:person rdf:type owl:Class;
                                     rdfs:subClassOf n2mu:agent.


### n2mu:popular-interest
n2mu:popular-interest rdf:type owl:Class;
                                              owl:equivalentClass [ owl:intersectionOf ( n2mu:interest
                                                                                         [ rdf:type owl:Restriction ;
                                                                                           owl:onProperty n2mu:like-by ;
                                                                                           owl:minQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                                                                           owl:onClass n2mu:agent
                                                                                         ]
                                                                                       ) ;
                                                                    rdf:type owl:Class
                                                                  ] ;
                                              rdfs:subClassOf n2mu:interest.


### n2mu:Robot
n2mu:robot rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:agent.


### n2mu:Sensor
n2mu:sensor rdf:type owl:Class;
                                     owl:equivalentClass <http://purl.obolibrary.org/obo/MF_0000019> .


### n2mu:Smell
n2mu:smell rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:sensor.


### n2mu:Song
n2mu:song rdf:type owl:Class;
                                   rdfs:subClassOf n2mu:interest-occurrence.


### n2mu:Sound
n2mu:sound rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:sensor.


### n2mu:Sport
n2mu:sport rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:interest-field.


### n2mu:sports-team
n2mu:sports-team rdf:type owl:Class;
                                         rdfs:subClassOf n2mu:interest-occurrence.


### n2mu:Taste
n2mu:taste rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:sensor.


### n2mu:Temperature
n2mu:temperature rdf:type owl:Class;
                                          rdfs:subClassOf n2mu:sensor.


### n2mu:Touch
n2mu:touch rdf:type owl:Class;
                                    rdfs:subClassOf n2mu:sensor.


### n2mu:University
n2mu:university rdf:type owl:Class;
                                         rdfs:subClassOf n2mu:institution.


### n2mu:Visual
